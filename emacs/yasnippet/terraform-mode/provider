# -*- mode: snippet -*-
# name: provider
# key: provider
# --
# Setup our providers so that we have deterministic dependecy resolution.
terraform {
  required_version = ">= 0.12"
}

# Setup our providers so that we have deterministic dependecy resolution.
provider "aws" {
region  = "\${var.region}"
  version = "~> 2.25"
}

provider "local" {
  version = "~> 1.3"
}

provider "template" {
  version = "~> 2.1"
}
